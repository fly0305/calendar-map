{
  "name": "react-calendar-heatmap",
  "version": "1.6.3",
  "description": "A calendar heatmap component built on SVG, inspired by github's commit calendar graph.",
  "author": "PatientsLikeMe Engineering",
  "main": "./build/index.js",
  "style": "./build/styles.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/patientslikeme/react-calendar-heatmap.git"
  },
  "license": "MIT",
  "keywords": [
    "calendar",
    "react",
    "react-component",
    "svg"
  ],
  "scripts": {
    "build": "npm-run-all clean build:css build:js build:demo",
    "build:js": "cross-env NODE_ENV=production webpack",
    "build:css": "postcss src/styles.css -d build/ --no-map",
    "build:demo": "cross-env NODE_ENV=demo webpack",
    "clean": "rimraf build",
    "deploy:demo": "npm run build:demo && ./scripts/build_ghpages.sh",
    "lint": "eslint --ext .js,.jsx --cache . && echo \"eslint: no lint errors found\" || true",
    "prepublish": "npm run clean && npm run build",
    "test": "jest --coverage",
    "test:watch": "npm test -- --watch",
    "start": "npm-run-all --parallel start:js start:css",
    "start:js": "webpack-dev-server --progress --inline",
    "start:css": "postcss src/styles.css -d build/ --no-map --watch"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-register": "^6.0.0",
    "cross-env": "^5.0.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-react": "^7.0.0",
    "jest": "^22.0.0",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.0.0",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.0.0"
  }
}
